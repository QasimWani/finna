<script>

var $password = $("#passwordTyping");
var $confirm = $("#confirmingPassword");

var $hideShowPasswordForConfirmationLocalAccount = $("#showHideConfirmPassword");
$password.keyup(function(){
   if($password.val().length <= 5)
   {
      $hideShowPasswordForConfirmationLocalAccount.hide(729);
       console.log("Password hidden");
}
else
{
    $hideShowPasswordForConfirmationLocalAccount.css("opacity","1");
    $hideShowPasswordForConfirmationLocalAccount.css("margin-bottom","10px");
    $hideShowPasswordForConfirmationLocalAccount.show(696);
    console.log("Password shown!");
}
});
    
    function crossreferencecheckingforwhetherusermadeamistakeliketheoneisawforMITapplication()
    {
        var $email = $("#userEmailAddress").val().split("@")[0];
        if($("#usernameOfUserFromCrossSynchronizationOfEmailAddress").val() != $("#userEmailAddress"))
        {
            $("#usernameOfUserFromCrossSynchronizationOfEmailAddress").val($email);
        }
        
        
         if($password.val() !== $confirm.val() && $password.val().length >= 9)
         {
            Swal.fire({
                          type: 'error',
                          title: 'Cryptic Error',
                          text: 'Your Passwords do not match',
                          showConfirmButton: false,
                          showCancelButton: true,
                          cancelButtonText: 'Re-type password',
                          footer:'Confirm your password with the same password you used initially'
                        })
            return false;
         }
         else
         {
            return true;
         }
    }
</script>

